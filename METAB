			wait(0.1)
			local args = {
				[1] = "<stroke color=\"#00FF00\" thickness=\"100\">O\n-^--^-</stroke>",
				[2] = "booth"
			}
			game:GetService("ReplicatedStorage").Events.EditBooth:FireServer(unpack(args))
			wait(0.1)
			local args = {
